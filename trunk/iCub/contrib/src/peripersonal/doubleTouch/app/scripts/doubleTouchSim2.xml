<application>
<name>doubleTouch Simulator BIS</name>

<dependencies>
</dependencies>

    <module>
        <name>iCub_SIM</name>
        <node>alecive</node>
        <stdio></stdio>
        <tag>iCub_SIM</tag>
    </module>
    <module>
        <name>iCubGui</name>
        <parameters>--robot icubSim</parameters>
        <node>alecive</node>
        <tag>iCubGui</tag>
    </module>
    <module>
        <name>simCartesianControl</name>
        <parameters>--no_legs</parameters>
        <node>alecive</node>
        <tag>simCartesianControl</tag>
    </module>
    <module>
        <name>iKinCartesianSolver</name>
        <parameters>--context simCartesianControl/conf --part left_arm</parameters>
        <node>alecive</node>
        <stdio></stdio>
        <tag>iKinCartesianSolver_LEFT</tag>
    </module>
    <module>
        <name>iKinCartesianSolver</name>
        <parameters>--context simCartesianControl/conf --part right_arm</parameters>
        <node>alecive</node>
        <stdio></stdio>
        <tag>iKinCartesianSolver_RIGHT</tag>
    </module>
    <module>
        <name>dataSetPlayer</name>
        <node>alecive</node>
        <stdio></stdio>
        <tag>dataSetPlayer</tag>
    </module>
    <module>
        <name>pmpServer</name>
        <parameters>--robot icubSim --part right_arm --name right_pmp_server</parameters>
        <node>alecive</node>
        <tag>pmpServerRight</tag>
    </module>
    <module>
        <name>pmpServer</name>
        <parameters>--robot icubSim --part left_arm --name left_pmp_server</parameters>
        <node>alecive</node>
        <tag>pmpServerLeft</tag>
    </module>
    <module>
        <name>iKinGazeCtrl</name>
        <parameters>--from configSim.ini --saccades off --Tneck 1 --Teyes 0.5</parameters>
        <node>alecive</node>
        <tag>iKinGazeCtrl</tag>
    </module>

    <connection>
        <from>/skinManager/skin_events:o</from>
        <to>/doubleTouch/contacts:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/doubleTouch/icubgui_objects:o</from>
        <to>/iCubGui/objects</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/right_pmp_server/gui:o</from>
        <to>/iCubGui/objects</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/left_pmp_server/gui:o</from>
        <to>/iCubGui/objects</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/icubSim/inertial</from>
        <to>/iCubGui/inertial:i</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icubSim/left_arm/state:o</from>
        <to>/iCubGui/left_arm:i</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icubSim/right_arm/state:o</from>
        <to>/iCubGui/right_arm:i</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icubSim/torso/state:o</from>
        <to>/iCubGui/torso:i</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icubSim/head/state:o</from>
        <to>/iCubGui/head:i</to>
        <protocol>udp</protocol>
    </connection>
</application>
